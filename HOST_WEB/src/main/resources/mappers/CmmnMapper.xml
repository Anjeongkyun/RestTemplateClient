<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.daon.host.web.mapper.CmmnMapper">


	<select id="getCmmnCode" resultType="com.daon.host.web.vo.CmmnVo$CmmnCode">
		select  * from tb_c_code where use_yn="Y"

		<where>
			<if test=" req.useYn != null and req.useYn!= '' ">
				AND req.useYn= #{req.useYn}
			</if>
		</where>
	</select>
	
	
	
	<select id="getCmmnBase" resultType="com.daon.host.web.vo.CmmnVo$CmmnBase">
		select  * from tb_c_base

	</select>
	

</mapper>