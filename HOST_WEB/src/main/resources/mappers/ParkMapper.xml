<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.daon.host.web.mapper.ParkMapper">

	<select id ="getList" resultType="parkVo">
		SELECT 
			 apt_id
			,project_id
			,apt_nm
			,apt_address
			,rep_num
			,manager_nm
			,latitude
			,longitude
			,ins_id
			,ins_date
			,upd_id
			,upd_date
		FROM
			tb_o_complex			
	</select>
	
		
	<select id="getPark" resultType="com.daon.host.web.vo.ParkVo$Table">
		select * from tb_o_complex
		<where>
			<if test=" req.parkSeq != null and req.parkSeq != '' and req.parkSeq != 'AAA01' ">
				AND apt_id = #{req.parkSeq}
			</if>
		

		</where>
	</select>
	
</mapper>